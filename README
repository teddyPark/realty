CREATE TABLE `raw_apartment_trade`(
  `jicode` string, 
  `price` string, 
  `build_year` string, 
  `trade_year` string, 
  `trade_month` string, 
  `trade_day` string, 
  `street_addr_1` string, 
  `street_addr_2` string, 
  `street_addr_3` string, 
  `dong_addr_1` string, 
  `dong_addr_2` string, 
  `dong_addr_3` string, 
  `jibun` string, 
  `apart_nm` string, 
  `apart_floor` string, 
  `apart_size` string)
COMMENT 'Data about apartment trade history'
PARTITIONED BY ( 
  `trade_ym` string)
ROW FORMAT SERDE 
  'org.apache.hadoop.hive.serde2.OpenCSVSerde' 
WITH SERDEPROPERTIES ( 
  'field.delim'=',',
  'quoteChar'='\"',
  'serialization.format'=',') 
STORED AS INPUTFORMAT 
  'org.apache.hadoop.mapred.TextInputFormat' 
OUTPUTFORMAT 
  'org.apache.hadoop.hive.ql.io.HiveIgnoreKeyTextOutputFormat'
LOCATION
  'hdfs://metatron-cluster/user/hive/warehouse/realty.db/raw_apartment_trade'
TBLPROPERTIES (
  'last_modified_by'='metatron', 
  'last_modified_time'='1547164435', 
  'skip.header.line.count'='1', 
  'transient_lastDdlTime'='1547164435')


